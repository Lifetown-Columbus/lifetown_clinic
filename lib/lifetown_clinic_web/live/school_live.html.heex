<h1><%= @school.name %></h1>
<div class="results">
  <div class="panel">
  <h2>Students</h2>
  <h4>Total Count: <span><%= @student_count %></span></h4>
  <dl>
  <%= for i <- 6..0 do %> 
    <dt>Lesson <span><%= i %></span></dt>
    <dd :for={student <- Map.get(@students_by_progress, i, [])}>
      <div><.link navigate={"/student/#{student.id}"}><%= student.name %></.link></div>
    </dd>
  <% end %>
  </dl>
  </div>
</div>
